<?xml version="1.0" encoding="UTF-8"?>
<fileset-config file-format-version="1.2.0" simple-config="true">
    <fileset name="all" enabled="true" check-config-name="Sun Checks (Eclipse)" local="false">
        <file-match-pattern match-pattern="." include-pattern="true"/>
    </fileset>
    <filter name="FileTypesFilter" enabled="true">
        <filter-data value="java"/>
        <filter-data value="properties"/>
        <filter-data value="R.java"/>
    </filter>
    <filter name="WriteProtectedFiles" enabled="true"/>
</fileset-config>
<module name="Checker">
    <module name="com.harukizaemon.simian.SimianCheck"/>
        <property name="language" value="java"/>
    <module/>
</module>
